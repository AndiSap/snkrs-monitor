{
  "compilerOptions": {
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "inlineSourceMap": true,
    "inlineSources": true,
    "lib": ["dom", "es2017"],
    "module": "commonjs",
    "moduleResolution": "node",
    "newLine": "LF",
    "outDir": "javascript",
    "rootDir": "src",
    "target": "es6",
    "types": [
      "reflect-metadata"
    ]
  }
}
